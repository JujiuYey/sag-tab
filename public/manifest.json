{
  "manifest_version": 3,
  "name": "sag-tab",
  "version": "1.0.0",
  "description": "极简风格的浏览器新标签页扩展，帮助用户快速访问常用网站",
  "permissions": ["storage", "unlimitedStorage"],
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "icons": {
    "16": "icons/icon-16.png",
    "32": "icons/icon-32.png",
    "48": "icons/icon-48.png",
    "128": "icons/icon-128.png"
  }
}
